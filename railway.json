{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "source /opt/venv/bin/activate && gunicorn app:app --bind 0.0.0.0:$PORT"
  },
  "variables": {
    "FRONTEND_URL": "https://mcb.up.railway.app",
    "REACT_APP_API_BASE_URL": "https://mcb.up.railway.app",
    "REACT_APP_GOOGLE_API_KEY": "your_production_google_api_key_here"
  }
}